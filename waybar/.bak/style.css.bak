* {
	border: none;
	border-radius: 10;
    font-family: "JetbrainsMono Nerd Font" ;
	font-size: 15px;
	min-height: 10px;
}

window#waybar {
	border: 2px solid #ABE9B3 ;
	background: transparent;
	padding-bottom: 4px;
}

window#waybar.hidden {
	opacity: 0.2;
}

#workspaces button {
    box-shadow: none;
	  text-shadow: none;
    padding: 1px;
    border-radius: 8px;
    margin-top: 4px;
    margin-bottom: 4px;
    margin-left: 0px;
    padding-left: 4px;
    padding-right: 4px;
    margin-right: 0px;
    color: #EAB2C2;
    /* animation: ws_normal 20s ease-in-out 1; */
}

#workspaces button.persistent{
    color: #EBDDBC;
}

#workspaces button.active {
    color: #EBDDBC;
    margin-left: 1px; 
    padding-left: 4px;
    padding-right: 8px;
    margin-right: 1px;
}

#workspaces button.persistent:nth-child(2){
    color: #EAB2C2;
}

#workspaces button.persistent:nth-child(3){
    color: #83A6BF;
}

#workspaces button:hover {
    color: #D5A5AA;
    /* animation: ws_hover 20s ease-in-out 1; */
    /* transition: all 0.3s cubic-bezier(.55,-0.68,.48,1.682); */
}


#window {
	margin-top: 6px;
	padding-left: 10px;
	padding-right: 10px;
	border-radius: 10px;
	transition: none;
  color: transparent;
	background: transparent;
}

